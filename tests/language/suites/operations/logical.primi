// With standand booleans.
a = true and true; // true
b = true and false; // false
c = false and true; // false
d = false and false; // false
e = true or true; // true
f = true or false; // true
g = false or true; // true
h = false or false; // false

// With nulls.
i = null and true; // false
j = null and false; // false
k = true and null; // false
l = false and null; // false
n = null and null; // false
p = null or true; // true
o = null or false; // false
q = true or null; // true
r = false or null; // false
s = null or null; // false
