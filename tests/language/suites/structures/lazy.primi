// Lazy variables
arr = [1, 2, 3, 4, 5];
sum = lazy {
	result = 0;
	foreach (arr as item) {
		result = result + item;
	}
	return result;
}
result_1 = sum

// Lazy properties
arr.second_sum = lazy {
	result = 0;
	foreach (this as item) {
		result = result + item;
	}
	return result;
}
result_2 = arr.second_sum
