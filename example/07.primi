string = "Když začínáme myslet, nemáme k dispozici nic jiného než myšlenku v její čisté neurčenosti, neboť k určení již patří jedno nebo nějaké jiné, ale na začátku ještě nemáme žádné jiné...";
result = "";
tmp = "";
foreach (string as c) {
	tmp = c;
	if (c == "ě") { tmp = "e"; }
	if (c == "ž") { tmp = "z"; }
	if (c == "č") { tmp = "c"; }
	if (c == "í") { tmp = "i"; }
	result = result + tmp;
}
